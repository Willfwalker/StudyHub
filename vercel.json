{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "includeFiles": [
            "Services/**",
            "static/images/*.jpg",
            "static/images/*.png"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "env": {
      "PYTHONPATH": ".",
      "FLASK_ENV": "production"
    }
}